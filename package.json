
{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss style.css -w",
    "devserver":"live-server",
    "start":"npm run",

    
    "compile:sass": "sass sass/main.scss style.comp.css",
    "concat:css": "concat -o style.concat.css  icon-font.css style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o style.prefix.css",
    "compress:css": "sass style.prefix.css style.main.css  --style compressed"
    
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concat": "^1.0.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "sass": "^1.34.1"
  }
}
